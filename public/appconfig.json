{
  "id": "com.fsmappointment.appointmentmanager",
  "name": "FSM Appointment Manager",
  "description": "Manage field service appointment requests and customer bookings efficiently",
  "version": "0.0.1",
  "icon": "https://d3do02kvz5vver.cloudfront.net/vite.svg",
  "outlet": "SHELL_HOME_SCREEN",
  "url": "/",
  "roles": ["SERVICE_MANAGER", "FIELD_SERVICE_TECHNICIAN", "DISPATCHER"],
  "provider": "FSM Appointment Solutions",
  "permissions": {
    "data": ["READ", "WRITE"],
    "ui": ["SHELL_HOME_SCREEN"]
  },
  "useShellSDK": true,
  "outletPositions": { "application": "shell", "position": "fullScreen", "href": "/" },
  "routes": [
    {
      "path": "/",
      "title": "Dashboard"
    },
    {
      "path": "/instances", 
      "title": "Appointment Instances"
    },
    {
      "path": "/booking/:token",
      "title": "Customer Booking"
    }
  ]
}
